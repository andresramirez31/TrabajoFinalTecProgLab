<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.RegistrarBanquete">
   <children>
      <VBox prefHeight="500.0" prefWidth="400.0">
         <children>
            <GridPane>
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <VBox.margin>
                  <Insets top="150.0" />
               </VBox.margin>
               <children>
                  <Label text="Tipo de fiesta" />
                  <Label text="Nombre del salon" GridPane.rowIndex="1" />
                  <Button mnemonicParsing="false" onMouseClicked="#btGuardar_click" text="Guardar" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <ComboBox fx:id="opcionFiesta" onMouseClicked="#opcionFiesta_click" prefWidth="150.0" GridPane.columnIndex="1" />
                  <ComboBox fx:id="nomSalon" onMouseClicked="#nomSalon_click" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
         </children></VBox>
   </children>
</AnchorPane>
